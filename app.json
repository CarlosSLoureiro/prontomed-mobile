{
  "expo": {
    "name": "ProntoMed",
    "slug": "prontomed",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.prontomed.app",
      "supportsTablet": true,
      "infoPlist": {
        "NSCalendarsUsageDescription": "Esse aplicativo usa seu calend√°rio para agendar consultas.",
        "NSContactsUsageDescription": "Esse aplicativo usa sua lista de contatos para agendar consultas"
      }
    },
    "android": {
      "adaptiveIcon": {
        "package": "com.prontomed.app",
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [ "READ_CALENDAR", "WRITE_CALENDAR", "READ_CONTACTS" ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
